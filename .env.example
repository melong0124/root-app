# Database Connection (Prisma)
# Example: postgresql://USER:PASSWORD@HOST:PORT/DATABASE?schema=public
DATABASE_URL="postgresql://postgres:password@localhost:5432/asset_db?schema=public"

# Supabase Configuration
SUPABASE_URL="https://your-project-id.supabase.co"
# Publishable Key (새로운 방식 - sb_publishable_로 시작)
SUPABASE_PUBLISHABLE_KEY="sb_publishable_your-key-here"
# Secret Key (서버 전용 - sb_secret_로 시작, 선택사항)
# SUPABASE_SECRET_KEY="sb_secret_your-key-here"

# Session Secret (for cookie encryption)
# Generate a random string for production
SESSION_SECRET="your-session-secret-change-in-production"
